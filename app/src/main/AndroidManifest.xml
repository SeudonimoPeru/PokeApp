<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
	package="com.jhon.apppokemon">

	<uses-permission android:name="android.permission.INTERNET" />
	<application
		android:allowBackup="true"
		android:icon="@drawable/logo_icon"
		android:label="@string/app_name"
		android:roundIcon="@drawable/logo_icon"
		android:supportsRtl="true"
		android:theme="@style/Theme.AppPokemon">
		<activity
			android:name=".features.grupos.ActGrupos"
			android:exported="false"
			android:theme="@style/Theme.AppPokemon" />

		<activity
			android:name=".features.main.ActMain"
			android:exported="false"
			android:theme="@style/Theme.AppPokemon" />

		<activity
			android:name=".features.login.ActLogin"
			android:exported="false"
			android:theme="@style/Theme.AppPokemon" />



		<activity
			android:name=".features.splash.ActSplash"
			android:exported="true"
			android:theme="@style/Theme.App.Starting">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />
				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>
	</application>

</manifest>